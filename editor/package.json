{
    "name": "coppervm",
    "displayName": "CopperVM",
    "description": "CopperVM language support for Visual Studio Code.",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/Supercaly/coppervm"
    },
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "copper",
            "aliases": ["Copper", "copper"],
            "extensions": [".copper"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "copper",
            "scopeName": "source.copper",
            "path": "./syntaxes/copper.tmLanguage.json"
        }]
    }
}