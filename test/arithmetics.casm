integer: 
    ; add integers
    push 1
    push 2
    add
    print

    ; add signed integers
    push -1
    push 2
    add
    print

    ; subtract integers
    push 2
    push 1
    sub
    print

    ; subtract signed integers
    push -2
    push 1
    sub
    print

    ; multiply integers
    push 2
    push 1
    mul
    print

    ; multiply signed integers
    push -2
    push 1
    imul
    print

    ; divide integers
    push 3
    push 2
    div
    print

    ; divide signed integers
    push -2
    push 5
    idiv
    print

    ; modulo integers
    push 6
    push 2
    mod
    print

    ; modulo signed integers
    push -8
    push 5
    imod
    print

floating_point:
    ; add float
    push 1.0
    push 2.0
    fadd
    print

    ; add signed float
    push -1.0
    push 2.0
    fadd
    print

    ; subtract float
    push 2.0
    push 1.0
    fsub
    print

    ; subtract signed float
    push -2.0
    push 1.0
    fsub
    print

    ; multiply float
    push 2.0
    push 1.0
    fmul
    print

    ; multiply signed float
    push -2.0
    push 1.0
    fmul
    print

    ; divide float
    push 3.0
    push 2.0
    fdiv
    print

    ; divide signed float
    push -2.0
    push 5.0
    fdiv
    print
   
halt