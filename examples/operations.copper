integer: 
    ; add integers
    push 1
    push 2
    add
    print
    drop

    ; add signed integers
    push -1
    push 2
    add
    print
    drop

    ; subtract integers
    push 2
    push 1
    sub
    print
    drop

    ; subtract signed integers
    push -2
    push 1
    sub
    print
    drop

    ; multiply integers
    push 2
    push 1
    mul
    print
    drop

    ; multiply signed integers
    push -2
    push 1
    imul
    print
    drop

    ; divide integers
    push 3
    push 2
    div
    print
    drop

    ; divide signed integers
    push -2
    push 5
    idiv
    print
    drop

    ; modulo integers
    push 6
    push 2
    mod
    print
    drop

    ; modulo signed integers
    push -8
    push 5
    imod
    print
    drop

floating_point:
    ; add float
    push 1.0
    push 2.0
    fadd
    print
    drop

    ; add signed float
    push -1.0
    push 2.0
    fadd
    print
    drop

    ; subtract float
    push 2.0
    push 1.0
    fsub
    print
    drop

    ; subtract signed float
    push -2.0
    push 1.0
    fsub
    print
    drop

    ; multiply float
    push 2.0
    push 1.0
    fmul
    print
    drop

    ; multiply signed float
    push -2.0
    push 1.0
    fmul
    print
    drop

    ; divide float
    push 3.0
    push 2.0
    fdiv
    print
    drop

    ; divide signed float
    push -2.0
    push 5.0
    fdiv
    print
    drop
   
halt