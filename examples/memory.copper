%memory 0x48, 0x65, 0x6A, 0x6A, 0x6F
%entry main

main:
    ; read byte at 0
    push 0
    read
    print
    
    ; read byte at 2
    push 2
    read
    print
    
    ; write at 5
    push 48
    push 5
    write

    ; read byte at 5
    push 5
    read
    print

    halt